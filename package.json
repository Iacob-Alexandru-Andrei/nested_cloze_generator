{
  "name": "basic2cloze",
  "version": "0.1.0",
  "description": "Automatic basic to cloze",
  "scripts": {
    "build": "npm-run-all pack",
    "pack": "cd src && bestzip ../dist.zip * && cd .."
  },
  "keywords": [
    "anki",
    "anki-addon"
  ],
  "author": "trgk",
  "license": "AGPL3",
  "devDependencies": {
    "bestzip": "^2.1.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "npm-auto-version": "^1.0.0",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{py}": [
      "autopep8 -i"
    ]
  },
  "dependencies": {}
}
